{
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "vue",
    "html",
    "yaml",
    "json",
    "python"
  ],
  "coc.preferences.colorSupport": true,
  "coc.preferences.formatOnType": true,
  "tsserver.npm": "/usr/bin/npm",
  "tsserver.formatOnType": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "javascriptreact"
  },
  "emmet.excludeLanguages": ["markdown", "javascript"],
  "snippets.userSnippetsDirectory": "~/.config/nvim/snippets",
  "snippets.ultisnips.enable": false,
  "suggest.enablePreview": true,
  "suggest.maxPreviewWidth": 100,
  "diagnostic.maxWindowHeight": 20,
  "diagnostic.errorSign": "✖",
  "diagnostic.warningSign": "⚠",
  "diagnostic.infoSign": "●",
  "diagnostic.hintSign": "○",
  "python.pythonPath": "~/.pyenv/shims/python",
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "/home/murin/.local/bin/black",
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Path": "/home/murin/.local/bin/flake8"
}
